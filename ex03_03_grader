score = input("Enter Score: ")
try :
    iscr = float(score)
except :
    print("Enter numeric value")
    quit()

if iscr >= 0.0 and iscr <=1.0 :
    if iscr >= 0.9 :
        print("A")
    elif iscr >= 0.8 :
        print("B")
    elif iscr >=0.7 :
        print("C")
    elif iscr >= 0.6 :
        print("D")
    else:
        print("F")
else:
    print("Score out of range")
