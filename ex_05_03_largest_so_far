#setting variable to -1 as the smallest possible
largest_so_far = -1
print("Before", largest_so_far)
for the_num in [9,41,12,3,74,15] :
#checking if each of the ints is bigger then current largest so far
	if the_num > largest_so_far :
#setting new largest so far if bigger then previous one
		largest_so_far = the_num
	print(largest_so_far, the_num)
print("After", largest_so_far)