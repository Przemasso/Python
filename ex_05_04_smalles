#setting variable to None - Flag value
smallest = None
print('Before')
for the_num in [9, 41, 12, 3, 74, 15] :
#checking if each of the ints is smaller then current first run will change None to 9
    if smallest is None :
        smallest = the_num
    elif the_num < smallest :
	    smallest = the_num
    print(smallest, the_num)
print('After', smallest)